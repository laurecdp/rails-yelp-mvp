<div class='container'>

<h1> Create a restaurant</h1>

<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.select :category, collection: ['chinese', 'italian', 'japanese', 'french', 'belgian'] %>
  <%= f.submit %>
<% end %>

</div>